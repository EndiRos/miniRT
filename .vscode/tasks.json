{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "cc",
            "command": "cc",
            "args": [
                "-fdiagnostics-color=always",
                "-I${workspaceFolder}/include",
                "-g",
                "${workspaceFolder}/src/reserve.c",
                "${workspaceFolder}/src/free.c",
                "${file}",
                "${workspaceFolder}/lib/libft.a",
                "${workspaceFolder}/lib/libgnl.a",
                "-lm",
                "-o",
                "${workspaceFolder}/${fileBasenameNoExtension}",
                
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}